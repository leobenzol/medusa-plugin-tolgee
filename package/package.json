{
  "name": "medusa-plugin-tolgee",
  "description": "Medusa V2 Plugin to easily add translations to your models with Tolgee ",
  "author": "SteelRazor47",
  "version": "1.5.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteelRazor47/medusa-plugin-tolgee"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    ".": "./.medusa/server/src/modules/tolgee/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa-plugin-integration",
    "translation",
    "tolgee",
    "ecommerce",
    "medusa-plugin-cms",
    "medusa-v2",
    "medusa"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.11.2",
    "@medusajs/admin-shared": "^2.11.2",
    "@medusajs/cli": "2.11.2",
    "@medusajs/framework": "2.11.2",
    "@medusajs/js-sdk": "2.11.2",
    "@medusajs/medusa": "2.11.2",
    "@medusajs/test-utils": "2.11.2",
    "@medusajs/utils": "2.8.2",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.39",
    "@tanstack/react-query": "5.64.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "jest": "^30.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.30.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "yalc": "1.0.0-pre.53",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.7.0",
    "@medusajs/cli": "^2.7.0",
    "@medusajs/framework": "^2.7.0",
    "@medusajs/medusa": "^2.7.0",
    "@medusajs/test-utils": "^2.7.0",
    "react-i18next": "^13.5.0"
  },
  "dependencies": {
    "@medusajs/icons": "^2.11.2",
    "@medusajs/ui": "^4.0.26",
    "@tolgee/core": "^6.2.7",
    "@tolgee/react": "^6.2.7",
    "@tolgee/web": "^6.2.7",
    "axios": "1.10.0",
    "axios-cache-interceptor": "1.8.0",
    "bottleneck": "^2.19.5",
    "dataloader": "^2.2.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@3.2.1"
}
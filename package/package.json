{
  "name": "medusa-plugin-tolgee",
  "description": "Medusa V2 Plugin to easily add translations to your models with Tolgee ",
  "author": "SteelRazor47",
  "version": "1.4.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SteelRazor47/medusa-plugin-tolgee"
  },
  "files": [
    ".medusa/server"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./workflows": "./.medusa/server/src/workflows/index.js",
    "./.medusa/server/src/modules/*": "./.medusa/server/src/modules/*/index.js",
    ".": "./.medusa/server/src/modules/tolgee/index.js",
    "./providers/*": "./.medusa/server/src/providers/*/index.js",
    "./admin": {
      "import": "./.medusa/server/src/admin/index.mjs",
      "require": "./.medusa/server/src/admin/index.js",
      "default": "./.medusa/server/src/admin/index.js"
    }
  },
  "keywords": [
    "medusa-plugin-integration",
    "translation",
    "tolgee",
    "ecommerce",
    "medusa-plugin-cms",
    "medusa-v2",
    "medusa"
  ],
  "scripts": {
    "build": "medusa plugin:build",
    "dev": "medusa plugin:develop",
    "prepublishOnly": "medusa plugin:build"
  },
  "devDependencies": {
    "@medusajs/admin-sdk": "2.11.3",
    "@medusajs/admin-shared": "2.11.3",
    "@medusajs/cli": "2.11.3",
    "@medusajs/dashboard": "^2.11.3",
    "@medusajs/framework": "2.11.3",
    "@medusajs/icons": "2.11.3",
    "@medusajs/js-sdk": "2.11.3",
    "@medusajs/medusa": "2.11.3",
    "@medusajs/test-utils": "2.11.3",
    "@medusajs/ui": "4.0.27",
    "@medusajs/utils": "2.11.3",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^30.0.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@medusajs/admin-sdk": "^2.11.2",
    "@medusajs/cli": "^2.11.2",
    "@medusajs/framework": "^2.11.2",
    "@medusajs/icons": "^2.11.2",
    "@medusajs/medusa": "^2.11.2",
    "@medusajs/test-utils": "^2.11.2",
    "@medusajs/ui": "^4.0.26",
    "@tanstack/react-query": "^5.64.2",
    "axios": "^1.13.2",
    "react-i18next": "^13.5.0",
    "react-router-dom": "6.20.1"
  },
  "dependencies": {
    "@tolgee/core": "^6.2.2",
    "@tolgee/react": "^6.2.2",
    "@tolgee/web": "^6.2.2",
    "axios-cache-interceptor": "^1.8.3",
    "bottleneck": "^2.19.5",
    "dataloader": "^2.2.3"
  },
  "engines": {
    "node": ">=20"
  }
}
